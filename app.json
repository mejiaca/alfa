{
  "expo": {
    "name": "Alfa",
    "slug": "alfa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "clanapp",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "ios": {
      "bundleIdentifier": "com.levapan.alfa",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSContactsUsageDescription": "Esta app necesita acceso a tus contactos para invitar amigos.",
        "NSPhotoLibraryUsageDescription": "Esta app necesita acceso a tus fotos para que puedas seleccionar im√°genes."
      }
    },
    "android": {
      "permissions": ["READ_CONTACTS", "READ_MEDIA_IMAGES", "READ_EXTERNAL_STORAGE"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.levapan.alfa",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "clanapp",
            "host": "*"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "edgeToEdgeEnabled": true
    },
    "extra": {
      "eas": {
        "projectId": "556d4642-6d35-47e2-bfcb-2cc34869973b"
      }
    }
  }
}
